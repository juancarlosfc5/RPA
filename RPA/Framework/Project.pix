                      <PropertyValue z:Id="i245">
                        <PropertyName>CSCode</PropertyName>
                        <_dataType>Expression</_dataType>
                        <_expression>@"using OpenQA.Selenium;
using OpenQA.Selenium.Chrome;

class Program
{
    static void Main(string[] args)
    {
        // Alternativa integrando variable de Pix Studio
        var rutaReporte = $""Reportes\\{nameFileExcel}"";

        // 🟢 Definir ruta del folder de errores
        var folderErrores = @""C:\Users\Reyes\Downloads\RPA\Errores"";
        Directory.CreateDirectory(folderErrores); // Crea la carpeta si no existe

        // 🟢 Definir ruta del archivo de log
        var rutaErrorLog = Path.Combine(folderErrores, ""error_log.txt"");

        // 🟢 Validar existencia del archivo Excel
        if (!File.Exists(rutaReporte))
        {
            File.WriteAllText(rutaErrorLog, $""❌ ERROR: No se encontró el archivo Excel: {rutaReporte}"");
            Console.WriteLine(""No se encontró el archivo Excel."");
            return;
        }

        // 🟢 Validar existencia de chromedriver.exe
        var chromedriverPath = @""C:\WebDriver\bin\chromedriver.exe"";
        if (!File.Exists(chromedriverPath))
        {
            File.WriteAllText(rutaErrorLog, $""❌ ERROR: chromedriver.exe no existe en: {chromedriverPath}"");
            Console.WriteLine(""No se encontró chromedriver.exe."");
            return;
        }

        // 🟢 Definir nombre y ruta de la captura de pantalla
        var folderEvidencias = @""C:\Users\Reyes\Downloads\RPA\Evidencias"";
        Directory.CreateDirectory(folderEvidencias);
        var nameScreenShot = $""Evidencia_{DateTime.Now.ToString(""yyyy-MM-dd_HHmmss"")}.png"";
        var rutaEvidencias = Path.Combine(folderEvidencias, nameScreenShot);

        var options = new ChromeOptions();
        options.AddArgument(""--start-maximized"");
        options.AddArgument(""--no-sandbox"");
        options.AddArgument(""--disable-dev-shm-usage"");
        options.AddArgument(""--remote-debugging-port=9222"");
        options.AddArgument(@""--user-data-dir=C:\Users\Reyes\RPA_Profile"");

        using (var driver = new ChromeDriver(@""C:\WebDriver\bin"", options, TimeSpan.FromSeconds(120)))
        {
            try
            {
                driver.Navigate().GoToUrl(""https://form.jotform.com/251733906511050"");
                Thread.Sleep(3000);

                // 🟢 Campo: Nombre del colaborador
                var nombreId = driver.FindElement(By.XPath(""//label[contains(text(), 'Nombre del colaborador')]"")).GetAttribute(""for"");
                if (string.IsNullOrEmpty(nombreId))
                    throw new Exception(""❌ No se encontró el ID del campo 'Nombre del colaborador'"");
                var inputNombre = driver.FindElement(By.Id(nombreId));
                inputNombre.SendKeys(""Juan Flórez"");

                // 🟢 Campo: Fecha de generación del reporte
                var fechaId = driver.FindElement(By.XPath(""//label[contains(text(), 'Fecha de generación del reporte')]"")).GetAttribute(""for"");
                if (string.IsNullOrEmpty(fechaId))
                    throw new Exception(""❌ No se encontró el ID del campo 'Fecha de generación del reporte'"");
                var inputFecha = driver.FindElement(By.Id(fechaId));
                var fechaActualFormateada = DateTime.Now.ToString(""MM-dd-yyyy"");
                ((IJavaScriptExecutor)driver).ExecuteScript(""arguments[0].value = arguments[1];"", inputFecha, fechaActualFormateada);

                // 🟢 Campo: Comentario
                var comentarioId = driver.FindElement(By.XPath(""//label[contains(text(), 'Comentario')]"")).GetAttribute(""for"");
                if (string.IsNullOrEmpty(comentarioId))
                    throw new Exception(""❌ No se encontró el ID del campo 'Comentario'"");
                var inputComentario = driver.FindElement(By.Id(comentarioId));
                inputComentario.SendKeys(""Enviado automáticamente por RPA Pix Studio"");

                // 🟢 Campo: Cargar archivo
                var archivoId = driver.FindElement(By.XPath(""//label[contains(text(), 'Cargar reporte')]"")).GetAttribute(""for"");
                if (!string.IsNullOrEmpty(archivoId))
                {
                    var inputArchivo = driver.FindElement(By.Id(archivoId));
                    inputArchivo.SendKeys(rutaReporte);
                }
                else
                {
                    throw new Exception(""❌ No se pudo encontrar el campo de carga de archivo."");
                }

                Thread.Sleep(3000);

                // 🟢 Botón Enviar
                var botonEnviar = driver.FindElement(By.XPath(""//button[@type='submit']""));
                botonEnviar.Click();

                Thread.Sleep(3000);

                // 🟢 Captura de pantalla
                Screenshot captura = ((ITakesScreenshot)driver).GetScreenshot();
                byte[] bytes = captura.AsByteArray;
                File.WriteAllBytes(rutaEvidencias, bytes);

                Console.WriteLine(""✅ Formulario enviado correctamente y evidencia guardada."");
            }

            catch (Exception ex)
            {
                File.WriteAllText(rutaErrorLog, $""❌ ERROR EN AUTOMATIZACION:\n{ex}"");
                Console.WriteLine(""Error durante la automatización. Revisa el log."");
                throw;
            }
        }
    }
}
